# Porcupine / wake window
# Replace with your actual key locally in .env (kept out of git)
PORCUPINE_ACCESS_KEY=__REPLACE_WITH_YOUR_PORCUPINE_KEY__
# Default keyword packaged with the repo
PORCUPINE_KEYWORD_PATH=config/Hello-Eddie.ppn

# Orchestrator legacy HTTP wake endpoint (unused by default)
WAKE_URL=http://127.0.0.1:6060/signal/wake
WAKE_BIND=127.0.0.1
# Disabled by default; mic owns wake window via local flag
WAKE_PORT=0
WAKE_WINDOW_MS=20000
HUSH_CLOSES_WINDOW=1
HUSH_SILENT_ACK=0

# Riva / Ollama / Voice defaults (override in your local .env if needed)
RIVA_SPEECH_API=localhost:50051
OLLAMA_URL=http://127.0.0.1:11434
OLLAMA_MODEL=qwen2.5:7b-instruct-q4_K_M
VOICE_NAME=English-US.Male-1

# Enable wakewatch by default when keys are present
EDDIE_WAKEWATCH=1

# Logging / UI behavior
ACK_THRESHOLD_MS=950
EDDIE_OPEN_WAV=0
